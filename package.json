{
  "name": "jsheroes.io",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "linter": "eslint -c .eslintrc.json app pages --quiet",
    "dev": "node server.js",
    "build": "next build && node scripts/generateServiceWorker.js",
    "start": "NODE_ENV=production node server.js",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore --write '**/*.{js,css,md}'",
    "precommit": "lint-staged",
    "test:e2e": "cypress run",
    "test:e2e:debug": "cypress open",
    "lh": "lighthouse-ci https://jsheroes.io",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "google-map-react": "^1.0.5",
    "helmet": "^3.15.0",
    "intersection-observer": "^0.5.0",
    "next": "^7.0.2",
    "now": "^11.3.9",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "uuid": "^3.3.2",
    "what-input": "^5.1.1",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "cypress": "^3.0.3",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lighthouse-ci": "git+https://github.com/ebidel/lighthouse-ci.git",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "wait-on": "^2.1.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --config .prettierrc --ignore-path .prettierignore --write",
      "eslint -c .eslintrc.json --ignore-path .eslintignore --fix",
      "git add"
    ]
  },
  "snyk": true
}
